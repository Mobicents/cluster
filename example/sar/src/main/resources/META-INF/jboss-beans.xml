<?xml version="1.0" encoding="UTF-8"?>

<deployment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="urn:jboss:bean-deployer:2.0">

	<bean name="Mobicents.CacheClassLoader" class="java.lang.ClassLoader">
		<constructor factoryClass="org.mobicents.cluster.test.MCClusterTest" factoryMethod="getClassLoader" />
	</bean>
	<bean name="MCClusterTest" class="org.mobicents.cluster.test.MCClusterTest">
		<annotation>@org.jboss.aop.microcontainer.aspects.jmx.JMX(name="javax.slee:name=FTFTimerTest",exposedInterface=org.mobicents.cluster.test.MCClusterTestMBean.class,registerDirectly=true)
		</annotation>

		<property name="faultTolerantScheduler">
			<inject bean="FaultTolerantScheduler" />
		</property>
		<property name="ftfRegistration">
			<inject bean="Mobicents.FTFRegistration" />
		</property>
		<property name="mobicentsCache">
			<inject bean="Mobicents.Cache" />
		</property>
		<property name="resourceGroup">
			<inject bean="Mobicents.FTF.ResourceGroup" />
		</property>
		<property name="jta">
			<inject bean="RealTransactionManager" />
		</property>
		<demand>Mobicents.FTFRegistration</demand>
		<demand>Mobicents.Cache</demand>
		<demand>Mobicents.FTF.ResourceGroup</demand>
		<demand>FaultTolerantScheduler</demand>
		<demand>jboss:service=TransactionManager</demand>
	</bean>




</deployment>
